let str='addition of two number ';
let str1=" and ";
let str3=" = ";
let num1=10;
let num2=20;
let num3=num1+num2;
let s=str+num1+str1+num2+str3+num3;
console.log(additon of two numbers);


To avoid above things we use :
------------------------------
let num1 = 10;
let num2 = 20;
console.log(`addition of two number ${num1} and ${num2} = ${num1 + num2}`); -> it is pactic type

let xtr = null;
console.log(xtr);

const sece= "college";
console.log(sece);


